    "directory": "packages/components/date-input"
  },
  "bugs": {
    "url": "https://github.com/nextui-org/nextui/issues"
  },
  "peerDependencies": {
    "@nextui-org/system": ">=2.4.0",
    "@nextui-org/theme": ">=2.4.0",
